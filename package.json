    "@reduxjs/toolkit": "^1.9.7",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.5",
    "@types/react-redux": "^7.1.28",
    "@types/redux-thunk": "^2.1.0",
    "apollo-server": "^3.13.0",
    "axios": "^1.5.1",
    "dotenv": "^16.3.1",
    "graphql": "^16.8.1",
    "graphql-tools": "^9.0.0",
    "jetifier": "^2.0.0",
    "mongoose": "^8.0.1",
    "react": "^18.2.0",
    "react-native": "0.72.5",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-safe-area-context": "^4.8.1",
    "react-native-screens": "^3.29.0",
    "react-redux": "^8.1.3",
    "redux": "^4.2.1",
    "redux-saga": "^1.2.3",
    "redux-thunk": "^2.4.2",
    "ts-jest": "^29.1.1",
    "uuid": "^9.0.1"
