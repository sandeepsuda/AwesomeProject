{
  "name": "AwesomeProject",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "nodemon --watch server --watch package.json server/index.js --exec babel-node --presets es2015,stage-2",
    "build": "tsc",
    "jest": "jest --verbose",
    "test": "jest",
    "iosBundle": "npx react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "prepAPK": "npx jetify && react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res"
  },
  "dependencies": {
    "@apollo/client": "^3.8.7",
    "@cucumber/gherkin": "^27.0.0",
    "@react-navigation/native": "^6.1.9",
