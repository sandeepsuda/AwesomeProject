    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "jest-cucumber": "^3.0.1",
    "metro-react-native-babel-preset": "0.76.8",
    "nodemon": "^3.0.1",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
